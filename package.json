{
  "name": "ai-testcase-analyzer",
  "version": "1.0.0",
  "description": "Playwright + TypeScript test automation framework with AI failure analysis and Slack notifications.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist coverage artifacts/*.log",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test",
    "playwright:install": "playwright install --with-deps",
    "analyze": "node dist/report-generator.js --ai-analysis",
    "cluster": "node dist/failure-clustering.js --advanced",
    "report": "node dist/report-generator.js",
    "prod": "cross-env NODE_ENV=production dotenv -e .env.prod -- npm run build && npm run test && npm run analyze",
    "prod:report": "npm run build && npm run analyze",
    "prod:full": "npm run clean && npm run prod",
    "setup": "node dist/setup-test.js",
    "ci": "npm run prod:full",
    "lint": "eslint src tests --ext .ts,.js",
    "lint:fix": "eslint src tests --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build && npm run test:unit"
  },
  "keywords": [
    "testing",
    "automation",
    "playwright",
    "ai",
    "analysis",
    "typescript",
    "qa"
  ],
  "author": "Rajni Nepali",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RajniNepaliVeltris/AI-TestcaseAnalyzer.git"
  },
  "bugs": {
    "url": "https://github.com/RajniNepaliVeltris/AI-TestcaseAnalyzer/issues"
  },
  "homepage": "https://github.com/RajniNepaliVeltris/AI-TestcaseAnalyzer#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@azure/openai": "^2.0.0",
    "@types/express": "^4.17.21",
    "axios": "^1.6.8",
    "chart.js": "^4.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "natural": "^7.0.3",
    "openai": "^4.47.1",
    "together-ai": "^0.7.0"
  }
}
