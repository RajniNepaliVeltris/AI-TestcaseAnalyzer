{
  "name": "ai-testcase-analyzer",
  "version": "1.0.0",
  "description": "Playwright + TypeScript test automation framework with AI failure analysis and Slack notifications.",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
  "all": "ts-node src/run-all.ts || ts-node src/report-generator.ts",
  "build": "tsc -p tsconfig.json",
    "analyze-failures": "ts-node src/ai-failure-analyzer.ts",
    "report": "ts-node src/report-generator.ts",
    "notify": "ts-node src/slack-notifier.ts",
    "ci": "npm run all && npm run analyze-failures && npm run notify"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "axios": "^1.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "dotenv": "^17.2.1"
  }
}
